<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data noupdate="1">

        <!-- HighFive Commission Product -->
        <record id="product_highfive_commission" model="product.product">
            <field name="name">HighFive Commission</field>
            <field name="type">service</field>
            <field name="categ_id" ref="product.product_category_all"/>
            <field name="sale_ok" eval="True"/>
            <field name="purchase_ok" eval="False"/>
            <field name="invoice_policy">order</field>
            <field name="default_code">HF-COMMISSION</field>
            <field name="list_price">0.00</field>
            <field name="standard_price">0.00</field>
            <field name="active" eval="True"/>
            <field name="description">Commission earned by HighFive on bookings</field>
        </record>

    </data>
</odoo>
